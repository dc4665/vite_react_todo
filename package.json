{
    "name": "vite_fast_react",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "start": "EXTEND_ESLINT=true react-scripts start",
        "build": "tsc && vite build && EXTEND_ESLINT=true react-scripts build",
        "serve": "vite preview",
        "test": "EXTEND_ESLINT=true react-scripts test"
    },
    "eslintConfig": {
        "extends": "react-app",
        "rules": {
            "no-unused-expressions": "off"
        }
    },
    "dependencies": {
        "@microsoft/fast-element": "^1.6.1",
        "@microsoft/fast-foundation": "^2.23.2",
        "@microsoft/fast-react-wrapper": "^0.1.9",
        "react": "^17.0.0",
        "react-dom": "^17.0.0"
    },
    "devDependencies": {
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@vitejs/plugin-react": "^1.0.0",
        "typescript": "^4.3.2",
        "vite": "^2.6.4"
    }
}
